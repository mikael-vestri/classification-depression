{
  "version": "2.0",
  "functionTimeout": "-1",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  },
  "extensions": {
    "http": {
        "routePrefix": "api",
        "maxOutstandingRequests": 2000,
        "maxConcurrentRequests": 1000
    }
}
}
